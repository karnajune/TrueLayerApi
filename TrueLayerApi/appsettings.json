{
  "Logging": {
    "Details": {
      "Format": "{Timestamp:yyyy-MM-ddTHH:mm:ss}Z|{Level}|{MachineName}|{SourceContext}|{Message:lj}{Exception}{NewLine}",
      "Path": "C:\\logs\\truelayerapi\\logs.txt"
    },   
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "PokemonCache": {
    "KeyName": "Translator",
    "MaxThreadCount": 2
  },
  "Shakespeare": {
    "Url": "https://funtranslations.com/api/shakespeare",
    "Api": "https://api.funtranslations.com/translate/shakespeare.json",
    "RetryCount": 2
  },
  "AllowedHosts": "*"
}
